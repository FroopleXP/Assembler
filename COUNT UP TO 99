; Click S to Step or Increase-Clock-Rate

START:

MOVW 0Xfd
MOVWR PORTA
CALL UNITS

MOVW 0X85
MOVWR PORTA
CALL UNITS

MOVW 0Xdb
MOVWR PORTA
CALL UNITS

MOVW 0Xcf
MOVWR PORTA
CALL UNITS

MOVW 0Xa7
MOVWR PORTA
CALL UNITS

MOVW 0Xee
MOVWR PORTA
CALL UNITS

MOVW 0Xfe
MOVWR PORTA
CALL UNITS

MOVW 0Xc5
MOVWR PORTA
CALL UNITS

MOVW 0Xff
MOVWR PORTA
CALL UNITS

MOVW 0Xe7
MOVWR PORTA
CALL UNITS

JMP START

UNITS:

MOVW 0X7d
MOVWR PORTA
NOP

MOVW 0X05
MOVWR PORTA
NOP

MOVW 0X5b
MOVWR PORTA
NOP

MOVW 0X4f
MOVWR PORTA
NOP

MOVW 0X27
MOVWR PORTA
NOP

MOVW 0X6e
MOVWR PORTA
NOP

MOVW 0X7e
MOVWR PORTA
NOP

MOVW 0X45
MOVWR PORTA
NOP

MOVW 0X7f
MOVWR PORTA
NOP

MOVW 0X67
MOVWR PORTA
NOP

RET

